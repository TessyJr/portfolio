{
  "extends": ["next/core-web-vitals", "plugin:prettier/recommended"],
  "plugins": ["prettier", "import"],
  "rules": {
    "prettier/prettier": "error",
    "import/order": ["error", { "alphabetize": { "order": "asc" } }],
    "no-unused-vars": "warn",
    "quotes": ["error", "single", { "avoidEscape": true }]
  }
}
